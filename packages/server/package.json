{
  "name": "@qtscraper/server",
  "version": "1.0.0",
  "description": "back end code for apollo grpahql server + sequelize",
  "main": "index.js",
  "author": "Sungwon Cho",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/node": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "dotenv": "^6.2.0",
    "nodemon": "^1.18.8",
    "rimraf": "^2.6.2"
  },
  "scripts": {
    "develop": "nodemon --exec babel-node -r node_modules/dotenv/config src",
    "build": "rimraf dist && babel src --out-dir dist",
    "serve": "node dist/index.js",
    "start": "yarn build && yarn serve"
  },
  "workspaces": {
    "nohoist": [
      "dotenv",
      "dotenv/**"
    ]
  }
}
